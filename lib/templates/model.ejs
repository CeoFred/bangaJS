const mongoose = require("mongoose");
const Schema = mongoose.Schema;

const <%= $name.toCamelCase() %>Schema = new Schema(
     {
          name: {
               type: String,
               trim: true,
          }
     },
     {
          timestamps: true
     }
);


module.exports = mongoose.model("<%= $name.toCamelCase() %>", <%= $name.toCamelCase() %>Schema)
